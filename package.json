  "dependencies": {
    "lerna": "4.0.0"
  },
  "devDependencies": {
    "@gemunionstudio/eslint-config": "0.1.7",
    "@types/node": "14.17.9",
    "@types/react": "17.0.17",
    "@typescript-eslint/eslint-plugin": "4.29.1",
    "@typescript-eslint/parser": "4.29.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-import-resolver-ts": "0.4.2",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-standard": "4.1.0",
    "husky": "4.3.8",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">=10"
  },
  "license": "UNLICENSED",
  "name": "@gemunionstudio/framework-root",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/GemunIonStudio/framework"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "build": "lerna run build --stream",
    "clean": "sh scripts/clean.sh",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "git:push:prod": "git push origin master:production",
    "installdep": "sh scripts/install.sh",
    "kill": "sudo pkill -9 node",
    "lint": "lerna run lint --stream --parallel",
    "migration": "lerna exec --scope @gemunionstudio/framework-admin-api -- npm run migration:run",
    "precommit": "lint-staged",
    "prettier": "prettier \"packages/**/*.{ts,tsx}\" \"services/**/*.{ts,tsx}\" --write",
    "registry": "lerna publish",
    "release": "lerna publish --exact --conventional-commits --registry=https://npm.pkg.github.com/",
    "release:git": "lerna publish from-package --yes --exact --conventional-commits --ignore-scripts --registry=https://npm.pkg.github.com/",
    "start": "lerna exec --parallel --scope @gemunionstudio/framework-admin-api --scope @gemunionstudio/framework-public-api --scope @gemunionstudio/framework-admin-ui --scope @gemunionstudio/framework-public-ui --scope @gemunionstudio/framework-emailer --scope @gemunionstudio/framework-warehouse -- npm start",
    "start:prod": "lerna exec --parallel --scope @gemunionstudio/framework-admin-api --scope @gemunionstudio/framework-public-api --scope @gemunionstudio/framework-emailer -- npm run start:prod",
    "test": "sh scripts/test.sh"
  },
  "version": "0.0.1"
}
