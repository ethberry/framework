  "dependencies": {
    "lerna": "4.0.0"
  },
  "devDependencies": {
    "@gemunion/eslint-config": "0.1.10",
    "@types/node": "16.11.13",
    "@types/react": "17.0.37",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-import-resolver-ts": "0.4.2",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "24.7.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.2.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-standard": "4.1.0",
    "husky": "4.3.8",
    "lint-staged": "12.1.2",
    "prettier": "2.5.1",
    "typescript": "4.5.4"
  },
  "engines": {
    "node": ">=12.13"
  },
  "license": "UNLICENSED",
  "name": "@gemunion/framework-root",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/gemunion/framework"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "build": "lerna run build --stream",
    "clean": "sh scripts/clean.sh",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "git:push:prod": "git push origin master:production",
    "installdep": "sh scripts/install.sh",
    "kill": "sudo pkill -9 node",
    "lint": "lerna run lint --stream --parallel",
    "migration": "lerna exec --scope @gemunion/framework-admin-api -- npm run migration:run",
    "precommit": "lint-staged",
    "prettier": "prettier \"packages/**/*.{ts,tsx}\" \"services/**/*.{ts,tsx}\" \"microservices/**/*.{ts,tsx}\" --write",
    "registry": "lerna publish",
    "release": "lerna publish --exact --conventional-commits --registry=https://npm.pkg.github.com/",
    "release:git": "lerna publish from-package --yes --exact --conventional-commits --ignore-scripts --registry=https://npm.pkg.github.com/",
    "start": "lerna exec --parallel --scope @gemunion/framework-admin-api --scope @gemunion/framework-public-api --scope @gemunion/framework-admin-ui --scope @gemunion/framework-public-ui --scope @gemunion/framework-tpl --scope @gemunion/framework-pdf --scope @gemunion/framework-eml -- npm start",
    "start:prod": "lerna exec --parallel --scope @gemunion/framework-admin-api --scope @gemunion/framework-public-api --scope @gemunion/framework-tpl --scope @gemunion/framework-pdf --scope @gemunion/framework-eml -- npm run start:prod",
    "test": "sh scripts/test.sh"
  },
  "version": "0.0.1"
}
