  "scripts": {
    "build": "tsc",
    "e2e": "NODE_ENV=${NODE_ENV:=test} jest --config=test/jest.config.json",
    "entity:create": "npm run typeorm -- entity:create -n",
    "lint": "eslint . --fix",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:run": "npm run typeorm migration:run",
    "prebuild": "rimraf dist/",
    "prepublish": "npm run build",
    "start": "cross-env NODE_ENV=${NODE_ENV:=development} tsc-watch --onSuccess \"node .\" --onFailure \"echo Compilation Failed\"",
    "start:prod": "cross-env NODE_ENV=${NODE_ENV:=production} node .",
    "start:stage": "cross-env NODE_ENV=${NODE_ENV:=staging} node .",
    "test": "cross-env NODE_ENV=${NODE_ENV:=test} jest --config jest.config.json --detectOpenHandles",
    "typeorm": "cross-env NODE_ENV=${NODE_ENV:=development} ts-node ./node_modules/.bin/typeorm --config server/ormconfig.ts"
  },
